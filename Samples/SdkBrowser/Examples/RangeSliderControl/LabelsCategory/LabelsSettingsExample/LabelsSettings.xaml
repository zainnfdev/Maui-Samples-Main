<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
             x:Class="SDKBrowserMaui.Examples.RangeSliderControl.LabelsCategory.LabelsSettingsExample.LabelsSettings">
    <VerticalStackLayout>
        <!-- >> rangeslider-labels-settings -->
        <telerik:RadRangeSlider x:Name="rangeSlider"
                                Minimum="0"
                                Maximum="100"
                                RangeStart="10"
                                RangeEnd="60"                                
                                TicksPlacement="End"
                                TickStep="5"
                                LabelsPlacement="End"
                                LabelStep="10" />
        <!-- << rangeslider-labels-settings -->
    </VerticalStackLayout>
</ContentView>
