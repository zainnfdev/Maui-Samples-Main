<?xml version="1.0" encoding="utf-8" ?>
<telerik:RadContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
                        x:Class="SDKBrowserMaui.Examples.BarcodeControl.PDF417Category.PDF417Example.PDF417"
                        Margin="5">
    <telerik:RadContentView.Content>
        <Grid>
            <!-- >> barcodepdf-example-xaml -->
            <telerik:RadBarcode x:Name="Barcode"                                 
                                   Value="Encoded data which will influence the rendered PDF417">
                <telerik:RadBarcode.Symbology>
                    <telerik:PDF417 SizingMode="Stretch"
                                Rows="30"
                                ECLevel="5"/>
                </telerik:RadBarcode.Symbology>
            </telerik:RadBarcode>
            <!-- << barcodepdf-example-xaml -->
        </Grid>
    </telerik:RadContentView.Content>
</telerik:RadContentView>