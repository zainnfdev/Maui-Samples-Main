<?xml version="1.0" encoding="utf-8" ?>
<telerik:RadContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
                        x:Class="SDKBrowserMaui.Examples.BarcodeControl.GettingStartedCategory.GettingStartedExample.BarcodeGettingStartedXaml"
                        Margin="5">
    <telerik:RadContentView.Content>
        <Grid RowDefinitions="3*,2*">
            <!-- >> barcode-gettingstarted -->
            <telerik:RadBarcode x:Name="barcode"
                     AutomationId="barcode"
                     Value="https://www.telerik.com/maui-ui">
                <telerik:RadBarcode.Symbology>
                    <telerik:QRCode SizingMode="Stretch" />
                </telerik:RadBarcode.Symbology>
            </telerik:RadBarcode>
            <!-- << barcode-gettingstarted -->
        </Grid>
    </telerik:RadContentView.Content>
</telerik:RadContentView>