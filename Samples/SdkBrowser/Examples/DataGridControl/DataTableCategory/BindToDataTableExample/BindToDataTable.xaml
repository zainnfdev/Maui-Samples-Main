                        xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
                        xmlns:local="clr-namespace:SDKBrowserMaui.Examples.DataGridControl.DataTableCategory"
                        x:Class="SDKBrowserMaui.Examples.DataGridControl.DataTableCategory.BindToDataTableExample.BindToDataTable">

    <!-- >> datagrid-datatable-binding -->
